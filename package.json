{
  "name": "nestjs-starter-project",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Nakul Sharma",
    "email": "nakul.165376@gmail.com",
    "url": "https://www.linkedin.com/in/nakul5harma/"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"{src,e2e-test,config}/**/*.{ts,js,json}\"",
    "lint": "eslint -c .eslintrc.js",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cpx \"resources/**\" \"dist/resources\" --verbose",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "prestart:prod": "npm install && npm run build",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main.js",
    "prestart:staging": "npm install && npm run build",
    "start:staging": "cross-env NODE_ENV=test node dist/src/main.js",
    "test": "cross-env NODE_ENV=development jest --detectOpenHandles --config ./jest.config.json",
    "test:watch": "cross-env NODE_ENV=development jest --watch --detectOpenHandles --config ./jest.config.json",
    "test:cov": "cross-env NODE_ENV=development jest --coverage --detectOpenHandles --config ./jest.config.json",
    "test:cov:staging": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --config ./jest.config.json",
    "test:e2e": "cross-env NODE_ENV=e2e-test jest --detectOpenHandles --config ./jest-e2e.config.json",
    "typeorm:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/options/typeorm-migration.options.ts",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/options/typeorm-migration.options.ts",
    "typeorm:staging": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/options/typeorm-migration.options.ts",
    "migration:generate": "npm run typeorm:dev migration:generate -- -n",
    "migration:run:dev": "npm run typeorm:dev migration:run",
    "migration:revert:dev": "npm run typeorm:dev migration:revert",
    "migration:run:prod": "npm run typeorm:prod migration:run",
    "migration:revert:prod": "npm run typeorm:prod migration:revert",
    "migration:run:staging": "npm run typeorm:staging migration:run",
    "migration:revert:staging": "npm run typeorm:staging migration:revert",
    "prepare": "husky install",
    "prettyquick": "pretty-quick --staged"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^1.4.1",
    "dotenv": "^16.0.1",
    "express-winston": "^4.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  }
}
